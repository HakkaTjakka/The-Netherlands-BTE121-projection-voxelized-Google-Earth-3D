@ECHO OFF

setlocal EnableExtensions EnableDelayedExpansion

IF NOT EXIST DONE-1 MKDIR DONE-1
IF NOT EXIST DONE0 MKDIR DONE0
IF NOT EXIST DONE1 MKDIR DONE1

SET /A COUNT=0
for /F "tokens=*" %%A in (HOLLAND_MCA.LST) do (
	SET /A COUNT=!COUNT! + 1
	IF EXIST E:\SAVES\TEST\REGION\DONE-1\%%A (
		COPY E:\SAVES\TEST\REGION\DONE-1\%%A DONE-1 > NUL
		ECHO !COUNT! E:\SAVES\TEST\REGION\DONE-1\%%A
	)
	IF EXIST E:\SAVES\TEST\REGION\DONE1\%%A (
		COPY E:\SAVES\TEST\REGION\DONE1\%%A DONE1> NUL
		ECHO !COUNT! E:\SAVES\TEST\REGION\DONE1\%%A
	)
	IF EXIST E:\SAVES\TEST\REGION\DONE0\%%A (
		COPY E:\SAVES\TEST\REGION\DONE0\%%A DONE0> NUL
		ECHO !COUNT! E:\SAVES\TEST\REGION\DONE0\%%A
	)
)

PAUSE